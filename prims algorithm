#include <stdio.h>
#define V 5
#define INF 999

void prim(int graph[V][V]) {
    int selected[V] = {0};
    selected[0] = 1;

    printf("Edge\tWeight\n");
    for (int e = 0; e < V - 1; e++) {
        int min = INF, u = -1, v = -1;
        for (int i = 0; i < V; i++)
            if (selected[i])
                for (int j = 0; j < V; j++)
                    if (!selected[j] && graph[i][j] && graph[i][j] < min) {
                        min = graph[i][j]; u = i; v = j;
                    }
        printf("%d - %d\t%d\n", u, v, min);
        selected[v] = 1;
    }
}

int main() {
    int graph[V][V] = {
        {0,2,0,6,0},
        {2,0,3,8,5},
        {0,3,0,0,7},
        {6,8,0,0,9},
        {0,5,7,9,0}
    };
    prim(graph);
    return 0;
}
